{
	"name": "rekogniz-ai",
	"version": "1.0.0",
	"description": "You don't have to try to recognize an actor in a film you're watching again.Face detector is an  app that gets movie data and poster based on user input then scrapes the web for images of actors and trains a neural network to recognize the scraped images on the movie poster!",
	"scripts": {
		"start": "cross-env NODE_ENV=production && build && serve:build",
		"dev": "cross-env NODE_ENV=development && watch:dev",
		"watch:dev": "webpack-dev-server --open",
		"build": "webpack",
		"serve:build": "http-server ./public -c-1 --cors --proxy http://localhost:8080?"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danBamikiya/rekogniz-ai.git"
	},
	"keywords": [
		"face-detector",
		"ai",
		"web-scraper",
		"neural-networks"
	],
	"author": "Dan Bamikiya",
	"license": "MIT",
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"@babel/runtime": "^7.12.5",
		"cheerio": "^1.0.0-rc.9",
		"core-js": "^3.8.2",
		"http-server": "^0.12.3",
		"selector-observer": "^2.1.6"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-transform-async-to-generator": "^7.13.0",
		"@babel/preset-env": "^7.12.11",
		"@types/dotenv-webpack": "^7.0.2",
		"babel-loader": "^8.2.2",
		"cross-env": "^7.0.3",
		"dotenv-webpack": "^7.0.1",
		"html-webpack-plugin": "^5.3.1",
		"path": "^0.12.7",
		"prettier": "2.3.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.2.4",
		"webpack": "^5.11.1",
		"webpack-cli": "^4.3.1",
		"webpack-dev-server": "^3.11.2"
	}
}

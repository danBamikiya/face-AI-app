{
	"name": "rekogniz-ai",
	"version": "1.0.0",
	"description": "You don't have to try to recognize an actor in a film you're watching again.Face detector is an  app that gets movie data and poster based on user input then scrapes the web for images of actors and trains a neural network to recognize the scraped images on the movie poster!",
	"scripts": {
		"start": "cd scripts && cross-env NODE_ENV=development BABEL_ENV='development' node bin/scripts.js start",
		"build": "cd scripts && cross-env NODE_ENV=production BABEL_ENV='production' node bin/scripts.js build",
		"serve:build": "http-server ./public -c-1 --cors --proxy http://localhost:8080?"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/danBamikiya/rekogniz-ai.git"
	},
	"keywords": [
		"face-detector",
		"ai",
		"web-scraper",
		"neural-networks"
	],
	"author": "Dan Bamikiya",
	"license": "MIT",
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"@babel/runtime": "^7.12.5",
		"cheerio": "^1.0.0-rc.9",
		"core-js": "^3.8.2",
		"http-server": "^0.12.3",
		"selector-observer": "^2.1.6"
	},
	"devDependencies": {
